{
  "kind": "collectionType",
  "collectionName": "diaries",
  "info": {
    "singularName": "diary",
    "pluralName": "diaries",
    "displayName": "Diary",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "feelings": {
      "type": "text"
    },
    "photos": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "startSleep": {
      "type": "time"
    },
    "endSleep": {
      "type": "time"
    },
    "weather": {
      "type": "string"
    },
    "remember": {
      "type": "boolean"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "diaries"
    },
    "companions": {
      "type": "enumeration",
      "enum": [
        "FAMILY",
        "FRIEND",
        "LOVER",
        "ACQUAINTANCE",
        "ALONE"
      ]
    },
    "body": {
      "type": "text"
    },
    "date": {
      "type": "date"
    },
    "mood": {
      "type": "enumeration",
      "enum": [
        "HAPPY",
        "GOOD",
        "SOSO",
        "BAD",
        "SAD"
      ]
